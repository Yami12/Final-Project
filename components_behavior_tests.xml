<flowes>
    <flow>
        <name>Trying to login without V mark on terms of use</name>
        <tests>
            <test>
                <name>Press the Child button</name>
                <resourceId>com.keepers:id/activity_terms_child_button</resourceId>
                <appActivity>com.keeper.common.TermsActivity</appActivity>
                <type>Button</type>
                <actionType>click</actionType>
                <expectedResult>toastMessage:Please accept the terms</expectedResult>
            </test>
        </tests>
    </flow>
    <flow>
        <name>child's age - above 16</name>
        <tests>
            <test>
                <name>Click on the empty check box - I accept the terms </name>
                <resourceId>com.keepers:id/check_i_accept</resourceId>
                <appActivity>com.keeper.common.TermsActivity</appActivity>
                <type>CheckBox</type>
                <actionType>click</actionType>
                <expectedResult>pass</expectedResult>
            </test>
            <test>
                <name>Press the Child button</name>
                <resourceId>com.keepers:id/activity_terms_child_button</resourceId>
                <appActivity>com.keeper.common.TermsActivity</appActivity>
                <type>Button</type>
                <actionType>click</actionType>
                <expectedResult>pass</expectedResult>
            </test>
            <test>
                <name>Insert hadar in the child's name box</name>
                <resourceId>com.keepers:id/input_child_name</resourceId>
                <appActivity>com.keeper.common.enter.child.bezeq.EnterChildActivity</appActivity>
                <type>Entry</type>
                <content>hadar</content>
                <expectedResult>pass</expectedResult>
            </test>
            <test>
                <name>Insert 05.11.2001 in the Birthday box</name>
                <resourceId>com.keepers:id/input_date_of_birth</resourceId>
                <appActivity>com.keeper.common.enter.child.bezeq.EnterChildActivity</appActivity>
                <type>Entry</type>
                <content>05.11.2010</content>
                <expectedResult>pass</expectedResult>
            </test>
            <test>
                <name>Enter yamushkach@gmail.com in the email box</name>
                <resourceId>com.keepers:id/input_email</resourceId>
                <appActivity>com.keeper.common.enter.child.bezeq.EnterChildActivity</appActivity>
                <type>Entry</type>
                <content>yamushkach@gmail.com</content>
                <expectedResult>pass</expectedResult>
            </test>
            <test>
                <name>Enter 123456 into the password box</name>
                <resourceId>com.keepers:id/input_password</resourceId>
                <appActivity>com.keeper.common.enter.child.bezeq.EnterChildActivity</appActivity>
                <type>Entry</type>
                <content>123456</content>
                <expectedResult>pass</expectedResult>
            </test>
            <test>
                <name>Click the Next button </name>
                <resourceId>com.keepers:id/button_proceed</resourceId>
                <appActivity>com.keeper.common.enter.child.bezeq.EnterChildActivity</appActivity>
                <type>Button</type>
                <actionType>click</actionType>
                <expectedResult>labelMessage:"Maximum age of child for parental use of keepers is 16</expectedResult>
            </test>
        </tests>
    </flow>
