<tests>
    <test>
        <name>Recive an offensive message</name>
        <supportedApps>WhatsApp Telegram Instagram</supportedApps>
        <text> kill you monkey fat </text>
        <isGroup>False</isGroup>
        <side>recive</side>
        <offensive>True</offensive>
    </test>
    <test>
        <name>Recive a message</name>
        <supportedApps>WhatsApp Telegram Instagram</supportedApps>
        <text>Hi Guy</text>
        <isGroup>False</isGroup>
        <side>recive</side>
        <offensive>False</offensive>
    </test>
    <test>
        <name>Send an offensive message</name>
        <supportedApps>WhatsApp Telegram Instagram</supportedApps>
        <text>fat guy</text>
        <isGroup>False</isGroup>
        <side>send</side>
        <offensive>True</offensive>
    </test>
    <test>
        <name>Send a message</name>
        <supportedApps>WhatsApp Telegram Instagram</supportedApps>
        <text>Hi guy</text>
        <isGroup>False</isGroup>
        <side>send</side>
        <offensive>False</offensive>
    </test>
    <test>
        <name>Removal from group</name>
         <supportedApps>WhatsApp</supportedApps>
        <groupName>check11</groupName>
    </test>
    <test>
        <name>Signing in is not allowed</name>
         <supportedApps>Chrome</supportedApps>
        <website_address>13tv.co.il</website_address>
    </test>
    <test>
        <name>device lock</name>
        <supportedApps></supportedApps>
    </test>
</tests>